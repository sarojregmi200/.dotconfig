configuration {
modes: "window,drun";
location: 0;
show-icons: true;
terminal: "alacritty";
icon-theme: "papirus";
drun-display-format: "{name}";
  timeout {
      action: "kb-cancel";
      delay:  0;
  }
  filebrowser {
      directories-first: true;
      sorting-method:    "name";
  }
}
@theme "current"
